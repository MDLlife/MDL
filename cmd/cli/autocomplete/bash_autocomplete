    two_word_flags+=("-f")
    local_nonpersistent_flags+=("--wallet-file=")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

_mdl-cli_walletOutputs()
{
    last_command="mdl-cli_walletOutputs"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()


    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

_mdl-cli_root_command()
{
    last_command="mdl-cli"

    command_aliases=()

    commands=()
    commands+=("addPrivateKey")
    commands+=("addressBalance")
    commands+=("addressGen")
    commands+=("addressOutputs")
    commands+=("blocks")
    commands+=("broadcastTransaction")
    commands+=("checkdb")
    commands+=("createRawTransaction")
    commands+=("decodeRawTransaction")
    commands+=("decryptWallet")
    commands+=("encryptWallet")
    commands+=("fiberAddressGen")
    commands+=("lastBlocks")
    commands+=("listAddresses")
    commands+=("listWallets")
    commands+=("send")
    commands+=("showConfig")
    commands+=("showSeed")
    commands+=("status")
    commands+=("transaction")
    commands+=("verifyAddress")
    commands+=("version")
    commands+=("walletAddAddresses")
    commands+=("walletBalance")
    commands+=("walletCreate")
    commands+=("walletDir")
    commands+=("walletHistory")
    commands+=("walletOutputs")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()


    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

__start_mdl-cli()
{
    local cur prev words cword
    declare -A flaghash 2>/dev/null || :
    declare -A aliashash 2>/dev/null || :
    if declare -F _init_completion >/dev/null 2>&1; then
        _init_completion -s || return
    else
        __mdl-cli_init_completion -n "=" || return
    fi

    local c=0
    local flags=()
    local two_word_flags=()
    local local_nonpersistent_flags=()
    local flags_with_completion=()
    local flags_completion=()
    local commands=("mdl-cli")
    local must_have_one_flag=()
    local must_have_one_noun=()
    local last_command
    local nouns=()

    __mdl-cli_handle_word
}

if [[ $(type -t compopt) = "builtin" ]]; then
    complete -o default -F __start_mdl-cli mdl-cli
else
    complete -o default -o nospace -F __start_mdl-cli mdl-cli
fi

